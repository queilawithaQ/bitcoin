<div class="hero section">
    <div class="container">
        <div class="row">
            <div class="main-content col-md-12">
                <div class="row">
                    <div class="hero-left order-2 col-lg-6 order-lg-1">
                        <h2>WebAuthn.io</h2>
                        <h5>A demo of the WebAuthn specification</h5>
                        <div class="mt-5 hero-form">
                            <div class="form-group">
                                <input type="email" class="form-control form-control-lg" id="input-email" placeholder="example_username">
                            </div>
                            <div class="input-group input-group-lg mb-3">
                                <div class="input-group-prepend">
                                    <label class="input-group-text" for="select-authenticator">Attestation Type</label>
                                </div>
